const json = `{
  "list": [
    {
      "name": "Petr",
      "age": "20",
      "prof": "mechanic"
    },
    {
      "name": "Vova",
      "age": "60",
      "prof": "pilot"
    }
  ]
}`;

const obj = JSON.parse(json);

console.log(obj.list);
console.log(obj.list[0].name);
console.log(obj.list[1].age);
