function countEvenOddZeroElements(arr) {
  var evenCount = 0;
  var oddCount = 0;
  var zeroCount = 0;

  for (var i = 0; i < arr.length; i++) {
    var element = arr[i];

    if (element === 0) {
      zeroCount++;
    } else if (typeof element === 'number' && !isNaN(element)) {
      if (element % 2 === 0) {
        evenCount++;
      } else {
        oddCount++;
      }
    }
  }

  console.log('Количество четных элементов:', evenCount);
  console.log('Количество нечетных элементов:', oddCount);
  console.log('Количество нулевых элементов:', zeroCount);
}

// Пример использования:
var array = [1, 2, 3, 0, 'a', null, 5, 6, 0, 'b', undefined, 8];
countEvenOddZeroElements(array);
